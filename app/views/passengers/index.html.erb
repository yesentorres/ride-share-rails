<h2>List of Passengers</h2>

<button>
  <%= link_to "Add Passenger", new_passenger_path %>
</button>

<ul>
  <% @passengers.each do |passenger|  %>
    <li>
      <h6><%= link_to passenger.name, passenger_path(passenger) %> (id <%= passenger.id %>)</h6>
      <p><%= passenger.phone_num %></p>
    </li>
  <% end %>
</ul>